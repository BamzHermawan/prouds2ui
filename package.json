{
	"name": "prouds2ui",
	"version": "1.0.0",
	"description": "Front-end HTML/CSS/JS for PRouDS",
	"scripts": {
		"bersih": "rm -rf dist",
		"demo": "yarn run mantap && cd release && php -S localhost:5501",
		"ngoding": "yarn run bersih && parcel src/login/login.html src/pages/*.html src/pages/setting/*.html -p 5500",
		"ngoding:pages": "parcel src/pages/*.html -d dist/pages -p 5500",
		"ngoding:login": "parcel src/login/login.html -d dist/login -p 5500",
		"mantap": "node bundler.js -o release/page -s release/src src/**/*.html"
	},
	"author": "PMO Telkomsigma",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@node-minify/core": "^4.1.2",
		"@node-minify/cssnano": "^4.1.2",
		"@node-minify/uglify-js": "^4.1.2",
		"@vue/component-compiler-utils": "^3.0.0",
		"cli-progress": "^2.1.1",
		"color-it": "^1.2.11",
		"dree": "^2.1.10",
		"http-server": "^0.11.1",
		"minimist": "^1.2.0",
		"parcel-bundler": "^1.12.3",
		"parcel-plugin-markdown": "^0.3.1",
		"parcel-plugin-markdown-string": "^1.3.5",
		"sass": "^1.21.0",
		"vue-template-compiler": "^2.6.10"
	},
	"marked": {
		"breaks": true,
		"pedantic": false,
		"gfm": true,
		"tables": true,
		"sanitize": false,
		"smartLists": true,
		"smartypants": false,
		"xhtml": false
	},
	"dependencies": {
		"axios": "^0.19.0",
		"buefy": "^0.7.10",
		"highcharts": "^7.1.2",
		"jsgantt-improved": "^2.2.11",
		"vue": "^2.6.10",
		"vue-clazy-load": "^0.4.2",
		"vue-hot-reload-api": "^2.3.3"
	},
	"alias": {
		"vue": "./node_modules/vue/dist/vue.common.js"
	}
}
