{
	"name": "prouds2ui",
	"version": "1.2.0",
	"description": "Front-end HTML/CSS/JS for PRouDS",
	"scripts": {
		"bersih": "rm -rf dist && rm -rf src/production/pages/dist && rm -rf src/development/pages/dist",
		"demo": "cd release && php -S localhost:5501",
		"ngoding": "yarn run bersih && parcel src/production/pages/**/*.html -p 5500",
		"ngoding:pick": "yarn run bersih && parcel -p 5500",
		"mantap": "node bundler.js -o release/page-new -s release/src-new src/production/pages/**/*.html",
		"build": "parcel build src/production/pages/**/*.html -d release/page-new --public-url /src-new --no-source-maps --no-minify"
	},
	"author": "PMO Telkomsigma",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@node-minify/core": "^4.1.2",
		"@node-minify/cssnano": "^4.1.2",
		"@node-minify/uglify-js": "^4.1.2",
		"@vue/component-compiler-utils": "^3.0.0",
		"bulma": "^0.8.0",
		"color-it": "^1.2.11",
		"dree": "^2.1.10",
		"http-server": "^0.11.1",
		"minimist": "^1.2.0",
		"parcel-bundler": "^1.12.3",
		"parcel-plugin-markdown": "^0.3.1",
		"parcel-plugin-markdown-string": "^1.3.5",
		"sass": "^1.21.0",
		"vue-template-compiler": "^2.6.10"
	},
	"marked": {
		"breaks": true,
		"pedantic": false,
		"gfm": true,
		"tables": true,
		"sanitize": false,
		"smartLists": true,
		"smartypants": false,
		"xhtml": false
	},
	"dependencies": {
		"axios": "^0.19.0",
		"buefy": "0.7.10",
		"compressorjs": "^1.0.6",
		"highcharts": "^7.1.2",
		"install": "^0.13.0",
		"loader.js": "^4.7.0",
		"moment": "^2.24.0",
		"perfect-scrollbar": "^1.4.0",
		"shepherd.js": "^4.2.0",
		"showdown": "^1.9.1",
		"vue": "^2.6.10",
		"vue-clazy-load": "^0.4.2",
		"vue-hot-reload-api": "^2.3.3"
	},
	"alias": {
		"vue": "./node_modules/vue/dist/vue.common.js",
		"bulma": "./node_modules/bulma/bulma.sass",
		"myGeeg": "./src/helper/geeg.js",
		"helper-moment": "./src/helper/moment.js",
		"helper-tools": "./src/helper/tools.js",
		"helper-loader": "./src/helper/loader.js",
		"helper-filter": "./src/helper/filters.js",
		"helper-tour": "./src/helper/tour.js",
		"helper-apis": "./src/helper/apis.js",
		"helper-tree-class": "./src/helper/treeClass.js",
		"components": "./src/components/registry.js"
	}
}
